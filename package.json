    "copy-dir": "^0.4.0",
    "dayjs": "^1.8.25",
    "debug": "^4.3.4",
    "delay": "^5",
    "dockerfile-generator": "^5.0.0",
    "dockerfile-utils": "^0.15.0",
    "dockerode": "^2.4.2",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.1.1",
    "glob": "^7.1.3",
    "ip": "^1.1.8",
    "is-class": "^0.0.9",
    "is-interactive": "^1",
    "is-online": "^9",
    "is-root": "^2",
    "js-yaml": "^3.4.6",
    "jsonfile": "^2.4.0",
    "listr2": "^6.6.1",
    "lodash": "^4.17.21",
    "log-update": "4.0.0",
    "nanoid": "^3",
    "node-cache": "^4.1.1",
    "npm-package-arg": "^11.0.1",
    "npm-profile": "^9.0.0",
    "object-hash": "^1.1.8",
    "pacote": "^15.1.3",
    "rimraf": "^4.1.1",
    "semver": "^7.3.2",
    "shelljs": "^0.8.4",
    "slugify": "^1.6.5",
    "string-argv": "0.1.1",
    "through": "^2.3.8",
    "valid-url": "^1.0.9",
    "winston": "2.4.5",
    "wrap-ansi": "7.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@lando/leia": "^1.0.0-beta.1",
    "@lando/vuepress-theme-default-plus": "1.0.0-beta.49",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-events": "^0.0.1",
    "command-line-test": "^1.0.10",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-vue": "^8.0.3",
    "mocha": "^9.1.2",
    "mock-fs": "https://github.com/pirog/mock-fs.git#256-ctxBindingAccess",
    "nyc": "^15.1.0",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0",
    "vuepress": "2.0.0-beta.53"
  },
  "bundledDependencies": [
